{
  "name": "udemy_cypress_13.6.6",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "open:cy": "cypress open",
    "run:cy": "cypress run",
    "smoke:cy": "cypress run --spec \"cypress\\e2e\\smoke\\*.cy.js\"",
    "test:cy": "cypress run --spec \"cypress\\e2e\\Tc15_CustomCommand.spec.cy.js\"",
    "browser:cy": "cypress run --browser chrome",
    "browser:headed:cy": "cypress run --browser chrome --headed",
    "headed:cy": "cypress run --headed",
    "dashboard:cy": "npx cypress run --record --key 9b15cfda-f84d-4bc8-bd7f-0244ee5ce53f",
    "scripts:all": "cypress run",
    "scripts:one": "cypress run --spec \"cypress\\e2e\\Tc01_FirstScript.spec.cy.js\"",
    "clean:reports:wn": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mocha && mkdir cypress\\reports\\mochareports",
    "clean:reports:lx": "rm -Rf cypress/reports && mkdir cypress/reports  && mkdir cypress/reports/mocha && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports:wn",
    "combine-reports": "mochawesome-merge ./cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "post-test": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts:one || npm run post-test",
    "report:ci": "npm run clean:reports && npm run dashboard:cy || true",
    "report:local": "npm run clean:reports && npm run report:ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.7.1",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-xpath": "^2.0.1",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "neat-csv": "^5.2.0",
    "xlsx": "^0.19.3"
  }
}
